{
  "manifest_version": 3,

  "name": "PR Chaser",
  "description": "This extension chases down github PRs where a +1 is needed",
  "version": "3.0",

  // https://developer.chrome.com/extensions/storage
  "permissions": [
    "storage",
    "https://github.com/*",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "./src/images/github.png",
    "default_popup": "popup.html"
  }
}